    "ng2-tel-input": "^1.0.14",
    "ng2-tree": "^2.0.0-rc.7",
    "ng2-uploader": "^2.0.0",
    "ng4-intl-phone": "^1.2.1",
    "ngx-bootstrap": "^2.0.2",
    "ngx-intl-tel-input": "^1.0.6",
    "photoswipe": "^4.1.1",
    "primeng": "github:matthieu-D/primeng",
